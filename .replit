# This file tells Replit how to run and build your project

# Specify the run command
run = "npm run dev"

# Specify the build command
build = "npm run build"

# Specify the entry point for your web server
web = "npm start"

# Optionally, specify the environment variables
env = {
  "PORT": "3000",
  "MONGODB_URI": "your_mongodb_uri_here"
}
